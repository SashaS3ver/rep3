<Window x:Class="Угадай.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Угадай"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Height="100" Margin="181,63,425,257">
            <TextBlock  Text="Введите число от 1 до 100"  TextWrapping="Wrap" FontSize="17" Margin="10,0,0,0"></TextBlock>
        </StackPanel>

        <StackPanel Height="100" Width="150" Margin="410,63,232,257">
            <TextBox PreviewTextInput="tbxNumber_PreviewTextInput" MaxLength="3" KeyDown="txtt1"  x:Name="tx1" Height="40" ></TextBox>
        </StackPanel>
        <StackPanel Width="300" Margin="246,168,246,10">
            <ListBox x:Name="list" Height="170"></ListBox>
            <Button x:Name="btn" Click="btnclick1" Content="Попробывать" Width="80" Height="31"></Button>
            
        </StackPanel>
    </Grid>
</Window>
